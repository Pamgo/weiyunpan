.alldom {
	width: 100%;
	height: 100%;
}

.alldom ul {
	padding: 0;
	list-style: none;
}

.alldom ul li {
	overflow: hidden;
	width: 100px;
	height: 115px;
	float: left;
	margin-left: 55px;
	background: url(../img/213125.png) center top no-repeat;
	border: 1px solid #fff;
	position: relative;
	transition: all 0.2s ease-in 0.1s;
}

.alldom ul li.bgclocrc {
	background: url(../img/213125.png) center top no-repeat #f1f2fd;
	border: 1px solid #a7aae3;
}

.alldom ul li:hover {
	background: url(../img/213125.png) center top no-repeat #f1f2fd;
	border: 1px solid #a7aae3;
}

.alldom input.changename {
	position: absolute;
	border-radius: 0px;
	left: -25px;
	bottom: 10px;
	overflow: hidden;
	width: 98px;
	height: 20px;
	color: #595c5f;
	line-height: 20px;
	text-align: center;
	font-size: 12px;
	z-index: 0;
	border: none;
}

.alldom input.changename:hover {
	background: none;
	color: #F33;
}

.list-view-container input {
	width: 88px;
	height: 28px;
	line-height: 20px;
	text-align: center;
	background: none;
	border: 1px solid #CCC;
	border-radius: 8px;
	transition: all 0.2s ease-in 0.1s;
	margin-left: 25px;
	cursor: pointer;
	float: left;
}

.list-view-container input:hover {
	background: #09F;
	color: #fff;
}

.menu-zdy {
	width: 120px;
	padding: 4px;
	height: auto;
	background: #f8f8f8;
	border: 1px solid #999;
	text-align: center;
	position: absolute;
	display: none;
}

.menu-zdy a {
	text-decoration: none;
	text-align: center;
	line-height: 24px;
	font-family: 'Microsoft YaHei';
	font-size: 12px;
	color: #666;
	transition: all 0.2s ease-in 0.1s;
}

.menu-zdy .menu-one {
	padding: 5px;
	width: 100px;
	height: 24px;
	border-bottom: 1px dashed #FC0;
}

.menu-zdy .menu-two {
	padding: 5px;
	width: 100px;
	height: 24px;
	border-bottom: 1px dashed #FC0;
}

.menu-zdy a:hover {
	padding: 3px 5px;
	color: #000;
	background: #fff;
}